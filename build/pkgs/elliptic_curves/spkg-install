#!/usr/bin/env bash
###########################################
## elliptic_curves
#########################################8#

cd cremona_mini
./spkg-install

EXIT_CODE=$?
if [ $EXIT_CODE -ne 0 ]; then
   echo "Error installing cremona mini database"
   exit $EXIT_CODE
fi

cd ../ellcurves
./spkg-install

EXIT_CODE=$?
if [ $EXIT_CODE -ne 0 ]; then
   echo "Error installing ellcurves database"
   exit $EXIT_CODE
fi
